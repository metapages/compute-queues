{
  "version": "2",
  "metadata": {
    "title": "Run a docker container",
    "author": "Dion Whitehead"
  },
  "allow": "clipboard-write",
  "inputs": {
    "metaframe/job": {
      "type": "json",
      "description": "The job definition to run"
    }
  },
  "outputs": {
    "job/result.json": {
      "type": "json",
      "description": "The result of the job"
    }
  },
  "hashParams": [
    "allowsetjob",
    "autostart",
    "config",
    "control",
    "debug",
    "definition",
    "ignoreQueueOverride",
    "inputs",
    "job",
    "maxJobDuration",
    "queue",
    "queueOverride",
    "resolverefs",
    "terminal"
  ]
}
