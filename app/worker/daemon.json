{
    "experimental": true,
    "ipv6": true,
    "ip6tables": true,
    "fixed-cidr-v6": "2001db8:1::/64",
    "default-address-pools": [
        {
            "base":10.100.0.116
            size": 24
        }
    ],
    "log-level": "debug",
    "features": {
        "buildkit": true
    },
    "hosts": [
        "unix:///var/run/docker.sock",
        "tcp://[::]:2375"
    ],
    "mtu": 1400,
    "max-concurrent-downloads": 10,
    "max-concurrent-uploads": 5,
    "metrics-addr": "0000:9323",
    "tls": false,
    "runtimes": {
        "nvidia": {
            "args": [],
            "path": "nvidia-container-runtime"
        }
    },
    "log-driver": "json-file",
    "log-opts": {
        "max-size": "10",
        "max-file": "3"
    }
}
