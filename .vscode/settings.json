{
  "files.exclude": {
    "**/.deno": true,
    "**/app/api/assets": true,
    "**/deno.lock": true,
    "**/dist": true,
    "**/.cache": true,
    "**/node_modules": true
  },
  "search.exclude": {
    "**/app/api/assets": true,
    "**/dist": true
  },
  "deno.enable": true,
  "deno.lint": true,
  "deno.disablePaths": ["app/browser"],
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "files.watcherExclude": {
    "**/dist": true
  },
  "typescript.enablePromptUseWorkspaceTsdk": true
}
